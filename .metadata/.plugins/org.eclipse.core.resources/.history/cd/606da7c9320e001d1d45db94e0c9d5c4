public class Demo {
	
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		RoundHole hole = new RoundHole(5);
		RoundPeg  rpeg = new RoundPeg(5);
		
		if(hole.fits(rpeg)) {
			System.out.println("Round peg r5 fits round hole r5");
		}


	}

}
