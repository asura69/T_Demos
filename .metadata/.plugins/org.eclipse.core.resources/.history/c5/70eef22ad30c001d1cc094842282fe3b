package com.org.tav.day7;

@FunctionalInterface
interface Myinterface{
	double getPiValue();
}

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
      //declare ref of myinterface 
		Myinterface  ref;
		ref = ()-> 3.14;
		System.out.println("Value of pi = "+ref.getPiValue());
		
	}

}
